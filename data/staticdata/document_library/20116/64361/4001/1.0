.s2of12 {
    float: left;
    width: 33%;
    padding: 10px 10px 0 0;
    margin: 0;
}



.btn-back:hover {
    color: #0067a1;
}

.icon {
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    display: inline-block;
    text-decoration: inherit;
    vertical-align: middle;
    margin-right: .35em;
}

.btn-back {
    color: #1b395f;
}

.icon-chevron_left:before, .ui-datepicker-prev span:before {
    content: "<";
}

/*Esto es lo que coloca las pastillas bien!!*/
form#form_makersLimited label.brand.errorToParent.fk-radio {
    height: 110px;
    width: 100%;
}


.fk-radio.brand {
    padding-right: 4px;
}
.fk-radio, .fk-check {
    padding: 10px 45px 10px 10px;
}

.funnel-header h1 {
    color: #244c7f;
    font-weight: 500;
    letter-spacing: -0.01em;
}


.fk-radio {
    border-radius: 5px;
}
.brand {
    text-align: center;
    padding: 12px 4px;
}
.fk-radio, .fk-check {
    position: relative;
    display: block;
    overflow: hidden;
    background: #FFF;
    padding: 12px;
    border: 1px solid #CCC;
    margin: 1px;
    font-size: 15px;
    text-align: left;
    color: #333;
    cursor: pointer;
}

.brand img {
    pointer-events: none;
    position: relative;
    display: block;
    margin: 0 auto 5px;
}

.fk-radio:before {
    vertical-align: middle;
    margin-right: .5em;    
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    display: inline-block;
    text-decoration: inherit;
    color: #244c7f;
}
.icon-chevron_right:before, .fk-radio:before, .ui-datepicker-next span:before {
    content: "";
}
.fk-radio:before, .fk-check:before {
    content: ' ';
    position: absolute;
    top: 12px;
    right: 12px;
    font-size: 20px;
}










		
		
		
		
		
		
		
		