.fk-radio, .fk-check {
    position: relative;
    display: block;
    overflow: hidden;
    background: #FFF;
    padding: 12px;
    border: 1px solid #CCC;
    margin: 1px;
    font-size: 15px;
    text-align: center;
    color: #333;
    cursor: pointer;
}

.brand img {
    pointer-events: none;
    position: relative;
    display: block;
    margin: 0 auto 5px;
}

.fk-radio:before {
    vertical-align: middle;
    margin-right: .5em;    
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    display: inline-block;
    text-decoration: inherit;
    color: #244c7f;
}


.icon-chevron_right:before, .fk-radio:before, .ui-datepicker-next span:before {
    content: ">";
}
.fk-radio:before, .fk-check:before {
    content: ' ';
    position: absolute;
    top: 12px;
    right: 12px;
    font-size: 20px;
}

